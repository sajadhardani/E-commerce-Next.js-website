generator client {
  provider = "prisma-client"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model Product {
  id          String @id @default(uuid())
  name        String
  category    String
  description String
  price       Float? @default(0.0)
  quantity    Int?   @default(0)
}
